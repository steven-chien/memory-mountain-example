set terminal png size 8192,4096 enhanced font "Helvetica,48"
set output 'memory_mountain.png'
set palette rgbformulae 33,13,10
set pm3d
set zlabel 'Throughput (MB/s)' rotate parallel
set xlabel 'Stride (x8 bytes)' rotate parallel
set ylabel 'Size' rotate parallel
splot '<tail -n +3 results.txt | head -n -1' matrix rowheaders columnheaders with lines notitle
exit
